[clase(top,none,[],[]),clase(animales,top,[pluricelulares],[]),clase(aves,animales,[vuelan],[]),clase(mamiferos,animales,[not(ponen_huevos)],[]),clase(ornitorrincos,mamiferos,[ponen_huevos,color=>nil],[]),clase(aguilas,aves,[carnivoros],[comen=>peces]),clase(pinguinos,aves,[not(vuelan),not(tamano=>largo)],[]),clase(primates,mamiferos,[],[]),clase(lemures,primates,[],[]),clase(chimpances,primates,[],[]),objeto([pete,peter],aguilas,[],[amigo=>arturo]),objeto([arturo,arthur],pinguinos,[listo,tamano=>largo],[amigo=>pete]),objeto([leo],aguilas,[],[]),objeto([gary,pingu],pinguinos,[alegre,bizarro,valiente],[]),objeto([orni,torri],ornitorrincos,[color=>negro],[]),objeto([juan,john,johnny],lemures,[],[]),objeto([joe,joel],chimpances,[],[]),objeto([pedro],primates,[],[]),objeto([george,jorge,mam],mamiferos,[ponen_huevos],[]),clase(caninos,mamiferos,[],[]),objeto([firulais],caninos,[muerde,raza=>labrador],[])].